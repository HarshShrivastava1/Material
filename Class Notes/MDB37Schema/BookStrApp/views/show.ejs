<%- include('header')%> 
<%- include('nav')%> 
<div class="showPage">
   <h1>Your Books</h1>
   <div class="midContentShPg">
         <% books.forEach(function(elem,index){ %>
            <h1> id: <%= elem.id %> </h1> 
            &nbsp;&nbsp;
            <h1> Name: <%= elem.name %>  </h1> 
            &nbsp;&nbsp;
            <h1> Author: <%= elem.author %>  </h1>
            &nbsp;&nbsp;
            <h1> Description: <%= elem.description %>  </h1> 
            &nbsp;&nbsp;
            <h1> Pages: <%= elem.pages %>  </h1> 
            &nbsp;&nbsp;
            <h1> Publication: <%= elem.publication %>  </h1>
            &nbsp;&nbsp; 
            <h1> Year: <%= elem.year %>  </h1> 
            &nbsp;&nbsp;
            <h1> Price: <%= elem.price %>  </h1> 
            &nbsp;&nbsp;
            <a href="/details/<%= index%>">Explore more</a> 
            <br><br><br><br><br>
         <% }) %> 
   </div>
</div>
<%- include('footer')%>



